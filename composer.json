{
  "name": "artapamudaid/secure-api-server",
  "version": "1.0.0",
  "description": "Laravel package for secure API HMAC authentication",
  "type": "library",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0 || ^11.0"
  },
  "autoload-dev": {
    "psr-4": {
      "SArtapamudaid\\ecureApiServer\\Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Artapamudaid\\SecureApiServer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Artapamudaid\\SecureApiServer\\SecureApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}